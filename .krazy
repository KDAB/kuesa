CHECKSETS qt5,c++,foss

#KDAB-specific checks
EXTRA kdabcopyright,kdabcommercial

#additional checks
#EXTRA defines,null

#exclude checks now being done by clazy or clang-tools
EXCLUDE strings,explicit,normalize,passbyvalue,operators,nullstrcompare,nullstrassign,doublequote_chars,qobject,sigsandslots,staticobjects,postfixop
#exclude more checks
EXCLUDE qminmax,captruefalse,typedefs,cpp
EXCLUDE style 

#no need to check 3rdparty stuff
SKIP /3rdparty/
SKIP /src/python/utils/utils\.py
SKIP /src/python/utils/pyside2_config\.py

#more stuff to skip
#skip templates
SKIP /qtc_templates/
SKIP /draco_prefix_p.h
SKIP /Manifest\.json
SKIP /shaders/graphs/
